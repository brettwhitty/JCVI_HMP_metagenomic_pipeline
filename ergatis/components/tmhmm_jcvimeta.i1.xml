<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>tmhmm compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>tmhmm</name>
            <state>incomplete</state>
            <executable>$;TMHMM_EXEC$;</executable>
            <arg>$;OTHER_OPTS$; $;I_FILE_PATH$;</arg>
            <param>  
                <key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw</value>
            </param>
            <param>  
                <key>stderr</key>
                <value>$;TMP_DIR$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.stderr</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>tmhmm2bsml</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/tmhmm2bsml</executable>
            <param>  
                <key>--input</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw</value>
            </param>
            <param>  
                <key>--output</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>  
                <key>--project</key>
                <value>$;PROJECT$;</value>
            </param>
            <param>  
                <key>--fasta_input</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>  
                <key>--compress_bsml_output</key>
                <value>$;COMPRESS_BSML_OUTPUT$;</value>
            </param>
            <param>
                <key>--id_repository</key>
                <value>$;PROJECT_ID_REPOSITORY$;</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>store config params</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/analysis2bsml</executable>
            <param>  
                <key>--ergatis_control_file</key>
                <value>$;ERGATIS_DIR$;.ini</value>
            </param>
            <param>  
                <key>--conf</key>
                <value>$;COMPONENT_CONFIG$;</value>
            </param>
            <param>  
                <key>--bsml_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>  
                <key>--componenturl</key>
                <value>$;WORKFLOW_REPOSITORY$;/component.xml</value>
            </param>
            <param>  
                <key>--pipelineurl</key>
                <value>$;PIPELINE_XML$;</value>
            </param>
            <param>  
                <key>--analysis_id</key>
                <value>$;COMPONENT_NAME$;_analysis</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>parse the bsml</name>
            <state>incomplete</state>
            <executable>/usr/local/projects/dacc/jcvi_metagenomic_autoannotate/bin/camera_parse_annotation_results_to_text_table.pl</executable>
            <param>
                <key>--input_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>
                <key>--input_type</key>
                <value>TMHMMBSML</value>
            </param>
            <param>
                <key>--output_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.bsml.parsed</value>
            </param>
            <param>
                <key>--work_dir</key>
                <value>/usr/local/projects/dacc/jcvi_metagenomic_autoannotate/data/</value>
            </param>
        </command>
    </commandSet>
</commandSetRoot>
